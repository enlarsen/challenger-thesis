\def\@makechapterhead#1{ \vspace*{2pc} {\centering
 \ifnum \c@secnumdepth >\m@ne \large \@chapapp{} \Thechapter \par
 \vspace{10pt} \fi \large\bf \uppercase\expandafter{#1}\par
 \nobreak \vspace{20pt}} }
